    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title"><%=panel_title%></h3>
      </div>
      <div class="panel-body">
        <ul class="list-group">
        <% events.each do |event| %>
          <li style="border: 0" class="list-group-item"><%= AMERICA_NEW_YORK_TIME_ZONE.format_date(event.start_time)%>: <%= link_to event.title, event %></li>
        <% end %>
        </ul>
      </div>
    </div>
