<%= form_for(@venue, html: { class: "form-horizontal" }) do |f| %>
  <% if @venue.errors.any? %>
		<div class="alert alert-dismissible alert-danger col-lg-12">
	  	<button type="button" class="close" data-dismiss="alert">Ã—</button>
      <ul>
      <% @venue.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="well bs-component col-lg-6">
	  <fieldset class="col-lg-12">
	    <legend>Venue</legend>
	    <div class="form-group">
        <label class="col-lg-3 control-label">Sync ID</label>
        <div class="col-lg-8">
          <%= f.number_field :rwu_id, class: "form-control", disabled: true %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :name, class: "col-lg-3 control-label" %>
        <div class="col-lg-8">        
          <%= f.text_field :name, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :street_address1, class: "col-lg-3 control-label" %>
        <div class="col-lg-8">        
          <%= f.text_field :street_address1, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :street_address2, class: "col-lg-3 control-label" %>
        <div class="col-lg-8">        
          <%= f.text_field :street_address2, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :unit, class: "col-lg-3 control-label" %>
        <div class="col-lg-8">        
          <%= f.text_field :unit, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :city, class: "col-lg-3 control-label" %>
        <div class="col-lg-8">        
          <%= f.text_field :city, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :state, class: "col-lg-3 control-label" %>
        <div class="col-lg-8">        
          <%= f.text_field :state, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :postal_code, class: "col-lg-3 control-label" %>
        <div class="col-lg-8">        
          <%= f.text_field :postal_code, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :url, class: "col-lg-3 control-label" %>
        <div class="col-lg-8">        
          <%= f.text_field :url, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :latitude, class: "col-lg-3 control-label" %>
        <div class="col-lg-8">        
          <%= f.text_field :latitude, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :longitude, class: "col-lg-3 control-label" %>
        <div class="col-lg-8">        
          <%= f.text_field :longitude, class: "form-control" %>
        </div>
      </div>
	    <div class="form-group">
	      <div class="col-lg-8 col-lg-offset-3">
	  	  	<%= link_to 'Cancel', venues_path, class: "btn btn-default" %>
		  	  <%= f.submit 'Save', class: "btn btn-primary" %>
		    </div>
			</div>
    </fieldset>
  </div>
<% end %>