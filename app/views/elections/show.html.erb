
<% if notice %>
	<div class="alert alert-dismissible alert-success col-lg-12">
		<button type="button" class="close" data-dismiss="alert">×</button>
		<%= notice %>
	</div>
<% end %>

<div class="well bs-component col-lg-6">
  <p>
    <strong>Sync ID:</strong>
    <%= @election.rwu_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= @election.name %>
  </p>

  <p>
    <strong>State:</strong>
    <%= @election.state %>
  </p>

  <p>
    <strong>Office type:</strong>
    <%= @election.office_type_id %>
  </p>

  <p>
    <strong>Special:</strong>
    <%= @election.special %>
  </p>

  <p>
    <strong>Election year:</strong>
    <%= @election.election_year %>
  </p>

  <ul class="pager">
  	<li class="next"><%= link_to 'Edit →', edit_election_path(@election) %></li>
  </ul>
</div>